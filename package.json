{
  "name": "predictype",
  "version": "0.6.1",
  "description": "PredicType is a library of pre-built and tested predicates for TypeScript, covering various data types and operations.",
  "homepage": "https://maduhaime.github.io/predictype/",
  "keywords": [
    "predicate",
    "typescript",
    "type-safe",
    "validation",
    "comparison",
    "membership",
    "range",
    "state"
  ],
  "license": "MIT",
  "author": "Marc-Antoine Duhaime",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maduhaime/predictype.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rimraf dist && npm run build:cjs && npm run build:esm",
    "code:pack": "npm run build && npm pack",
    "code:prepublish": "npm run build && npm test",
    "code:publish": "npm publish --access public",
    "docs": "rimraf docs && typedoc",
    "docs:deploy": "npm run docs && gh-pages -d docs -f",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "gh-pages": "^6.3.0",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
